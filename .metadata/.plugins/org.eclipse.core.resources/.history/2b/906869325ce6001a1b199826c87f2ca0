package fr.formation;

public abstract lass Participants {
	
	private String name;
	
	public Participants(String name) {
		if (name == null) {
			throw new NullPointerException("Name cannot be null!");
		}
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	
	 public abstract String play();
	

}
